<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LancementDuJeuController">
    <ImageView fx:id="imgFond" fitHeight="720.0" fitWidth="1280.0" layoutY="6.0" pickOnBounds="true"
               preserveRatio="true"/>
    <VBox layoutX="590.0" layoutY="251.0">
        <Button fx:id="btnJouerJeu" mnemonicParsing="false" onAction="#lancementChoixPerso" prefHeight="50.0"
                prefWidth="100.0" text="Jouer">
            <VBox.margin>
                <Insets bottom="25.0"/>
            </VBox.margin>
        </Button>
        <Button fx:id="btnParametreJeu" mnemonicParsing="false" onAction="#choixParametre" prefHeight="50.0"
                prefWidth="100.0" text="ParamÃ¨tres">
            <VBox.margin>
                <Insets bottom="25.0"/>
            </VBox.margin>
        </Button>
        <Button fx:id="btnQuitterJeu" alignment="CENTER" mnemonicParsing="false" onAction="#quitterJeu"
                prefHeight="50.0" prefWidth="100.0" text="Quitter">
            <VBox.margin>
                <Insets bottom="25.0"/>
            </VBox.margin>
        </Button>
    </VBox>
    <GridPane layoutX="540.0" layoutY="318.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
    </GridPane>
</AnchorPane>
